{
    "name":              "youmesoft/callr-bundle",
    "type":              "symfony-bundle",
    "description":       "",
    "keywords":          [
        "getrdv"
    ],
    "homepage":          "https://bitbucket.org/youmesoft/callr-api-symfonybundle",
    "license":           "MIT",
    "authors":           [
        {
            "name":  "Gatman Mihail",
            "email": "mgatman@youmesoft.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "require":           {
        "php":                          "^5.5.9|~7.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/config":               "~2.7|~3.0",
        "callr/sdk-php":                "0.*"
    },
    "require-dev":       {
