        "symfony/doctrine-bridge":  "~2.7|~3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm":             "^2.5",
        "doctrine/annotations":     "1.2.*",
        "symfony/yaml":             "~2.7|~3.0"
    },
    "autoload":          {
        "psr-4": {
            "Youmesoft\\CallrBundle\\": ""
